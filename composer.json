{
    "name": "elijahcruz/laravel-square",
    "description": "Package for Square API, built for Laravel",
    "keywords": [
        "elijahcruz",
        "laravel",
        "laravel-square",
        "SquareService"
    ],
    "autoload": {
        "psr-4": {
            "Elijahcruz\\LaravelSquare\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Elijah Cruz",
            "email": "eli@theecgaming.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-package-tools": "^1.17"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elijahcruz\\LaravelSquare\\LaravelSquareServiceProvider"
            ]
        },
        "aliases": {
            "LaravelSquare": "Elijahcruz\\LaravelSquare\\Facades\\LaravelSquare"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
